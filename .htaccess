Header set Access-Control-Allow-Origin "*"

RewriteEngine On    # Turn on the rewriting engine

RewriteCond %{HTTPS} !=on
RewriteRule ^.*$ https://%{SERVER_NAME}%{REQUEST_URI} [R,L]

RewriteRule    ^/?$    index.php    [NC,L]
RewriteRule    ^home/?$    index.php    [NC,L]  
RewriteRule    ^vacancy/?$    vacantHostelPage.php    [NC,L]
RewriteRule    ^about/?$    about.html    [NC,L]  
RewriteRule    ^terms/?$    terms.html    [NC,L]
RewriteRule    ^privacy/?$    privacy.html    [NC,L]
RewriteRule    ^order/?$    ride.php    [NC,L]
RewriteRule    ^agent/?$    agent-page.php    [NC,L]
RewriteRule    ^ticket/?$    ticket.php    [NC,L]

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php80” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php80 .php .php8 .phtml
</IfModule>
